<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="castle" type="Castle.Windsor.Configuration.AppDomain.CastleSectionHandler, Castle.Windsor"/>
  </configSections>
  <connectionStrings>
    <add name="connectionString" connectionString="Server=.\SQLEXPRESS;Database=Pet;Trusted_Connection=True;" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <appSettings>
    <add key="Assembly" value="Alfa.Pet.Model"/>
